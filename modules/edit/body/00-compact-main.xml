                                <xf:select1 ref="./@point">
                                    <xf:label>Point</xf:label>
                                    <xf:itemset nodeset="instance('code-tables')/code-table[code-table-name='date-point-code']/items/item">
                                        <xf:label ref="label"/>
                                        <xf:value ref="value"/>
                                    </xf:itemset>
                                </xf:select1>
                            </xf:group>
                            <xf:group class="text_10 hide-label margin-plus">
                                <xf:input ref=".">
                                    <xf:label>Copyright Date</xf:label>
                                </xf:input>
                            </xf:group>
                        </xf:repeat>
                    </fieldset>
                </xf:group>
            </xf:repeat>
        </fieldset>
    </xf:group>
