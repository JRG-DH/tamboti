                            <xf:label ref="label"/>
                            <xf:value ref="value"/>
                        </xf:itemset>
                    </xf:select1>
                </xf:group>
                <xf:group ref=".[@transliteration]" class="margin-plus">
                    <xf:select1 ref="/mods:mods/mods:extension/e:transliterationOfResource">
