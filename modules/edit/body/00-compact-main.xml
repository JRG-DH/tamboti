                        <xf:itemset nodeset="instance('code-tables')/code-table[code-table-name='transliteration-short-code']/items/item">
                            <xf:label ref="label"/>
                            <xf:value ref="value"/>
                        </xf:itemset>
                    </xf:select1>
                </xf:group>
                <xf:group ref=".[not(@script) and not(@transliteration) and (./mods:language/mods:scriptTerm = 'latn')]" class="margin-plus">
                    <xf:select1 ref="/mods:mods/mods:language/mods:scriptTerm">
                        <xf:label>Script<span class="xforms-help">
                                <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                <div class="xforms-help-value">
                                    <xf:output mediatype="application/xhtml+xml" incremental="true" ref="instance('code-tables')/code-table[code-table-name='hint-code']/items/item[value='script-compact']/help"/>
                                </div>
                            </span>
                        </xf:label>
                        <xf:itemset nodeset="instance('code-tables')/code-table[code-table-name='script-short-code']/items/item">
                            <xf:label ref="label"/>
                            <xf:value ref="value"/>
                        </xf:itemset>
                    </xf:select1>
                </xf:group>
                <xf:group class="text_10 margin-plus">
                    <xf:input ref="./mods:nonSort">
                        <xf:label>
                            <xf:group ref=".[string-length(../@type) = 0]">Non Sort<span class="xforms-help">
                                    <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                    <div class="xforms-help-value">
                                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="instance('code-tables')/code-table[code-table-name='hint-code']/items/item[value='nonSort']/help"/>
                                    </div>
                                </span>
                            </xf:group>
                            <xf:group ref=".[../@type='translated' and not(../@lang)]">Transliterated Non Sort<span class="xforms-help">
                                    <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                    <div class="xforms-help-value">
                                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="instance('code-tables')/code-table[code-table-name='hint-code']/items/item[value='nonSort']/help"/>
                                    </div>
                                </span>
                            </xf:group>
                            <xf:group ref=".[../@type='translated' and (../@lang)]">Translated Non
                                    Sort<span class="xforms-help">
                                    <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                    <div class="xforms-help-value">
                                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="instance('code-tables')/code-table[code-table-name='hint-code']/items/item[value='nonSort']/help"/>
                                    </div>
                                </span>
                            </xf:group>
                        </xf:label>
                    </xf:input>
                </xf:group>
                <xf:group class="text_150 margin-plus">
                    <xf:input ref="./mods:title">
                        <xf:label>
                            <xf:group ref=".[string-length(../@type) = 0]">Title<span class="xforms-help">
                                    <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                    <div class="xforms-help-value">
                                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="instance('code-tables')/code-table[code-table-name='hint-code']/items/item[value='title-compact']/help"/>
                                    </div>
                                </span>
                            </xf:group>
                            <xf:group ref=".[../@type='translated' and not(../@lang)]">Transliterated Title<span class="xforms-help">
                                    <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                    <div class="xforms-help-value">
                                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="instance('code-tables')/code-table[code-table-name='hint-code']/items/item[value='title-compact']/help"/>
                                    </div>
                                </span>
                            </xf:group>
                            <xf:group ref=".[../@type='translated' and (../@lang)]">Translated
                                    Title<span class="xforms-help">
                                    <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                    <div class="xforms-help-value">
                                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="instance('code-tables')/code-table[code-table-name='hint-code']/items/item[value='title-compact']/help"/>
                                    </div>
                                </span>
                            </xf:group>
                        </xf:label>
                    </xf:input>
                </xf:group>
                <xf:group class="text_150 margin-plus">
                    <xf:input ref="./mods:subTitle">
                        <xf:label>
                            <xf:group ref=".[string-length(../@type) = 0]">Subtitle<span class="xforms-help">
                                    <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                    <div class="xforms-help-value">
                                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="instance('code-tables')/code-table[code-table-name='hint-code']/items/item[value='subTitle-compact']/help"/>
                                    </div>
                                </span>
                            </xf:group>
                            <xf:group ref=".[../@type='translated' and not(../@lang)]">Transliterated Subtitle<span class="xforms-help">
                                    <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                    <div class="xforms-help-value">
                                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="instance('code-tables')/code-table[code-table-name='hint-code']/items/item[value='subTitle-compact']/help"/>
                                    </div>
                                </span>
                            </xf:group>
                            <xf:group ref=".[../@type='translated' and (../@lang)]">Translated
                                    Subtitle<span class="xforms-help">
                                    <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                    <div class="xforms-help-value">
                                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="instance('code-tables')/code-table[code-table-name='hint-code']/items/item[value='subTitle-compact']/help"/>
                                    </div>
                                </span>
                            </xf:group>
                        </xf:label>
                    </xf:input>
                </xf:group>
            </xf:repeat>
        </fieldset>
    </xf:group>
    <xf:group ref=".[string(mods:extension/e:template) = 'monograph-latin'] | .[string(mods:extension/e:template) = 'monograph-transliterated'] | .[string(mods:extension/e:template) = 'edited-volume-latin'] | .[string(mods:extension/e:template) = 'edited-volume-transliterated'] | .[string(mods:extension/e:template) = 'periodical-latin'] | .[string(mods:extension/e:template) = 'periodical-transliterated']">
        <fieldset class="level-1">
            <div class="join">
                <span class="legend">Origin Information</span>
                <span class="xforms-help">
                    <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                    <div class="xforms-help-value">
                        <xf:output mediatype="application/xhtml+xml" incremental="true" ref="instance('code-tables')/code-table[code-table-name='hint-code']/items/item[value='originInfo-compact']/help"/>
                    </div>
                </span>
            </div>
            <xf:repeat nodeset="./mods:originInfo" id="origininfo-repeat">
                <xf:group ref=".[string-length(@transliteration) &gt; 0]">
                    <br/>
                </xf:group>
                <xf:group ref=".[mods:place]">
                    <fieldset class="level-2">
                        <div class="join">
                            <span class="legend">City</span>
                            <span class="xforms-help">
                                <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                <div class="xforms-help-value">
                                    <xf:output mediatype="application/xhtml+xml" incremental="true" ref="instance('code-tables')/code-table[code-table-name='hint-code']/items/item[value='place-compact']/help"/>
                                </div>
                            </span>
                        </div>
                        <xf:repeat nodeset="./mods:place" id="place-repeat">
                            <xf:group ref=".[mods:placeTerm]">
                                <fieldset class="level-3">
                                    <xf:repeat nodeset="./mods:placeTerm" id="placeTerm-repeat">
                                        <xf:group ref=".[not(@transliteration)]" class="margin-plus text_100 hide-label">
                                            <xf:input ref=".">
                                                <xf:label>City Name</xf:label>
                                            </xf:input>
                                        </xf:group>
                                        <xf:group ref=".[@transliteration]" class="margin-plus text_100">
                                            <xf:input ref=".">
                                                <xf:label>Transliterated</xf:label>
                                            </xf:input>
                                        </xf:group>
                                    </xf:repeat>
                                </fieldset>
                            </xf:group>
                        </xf:repeat>
                    </fieldset>
                </xf:group>
                <xf:group ref=".[mods:publisher]">
                    <fieldset class="level-2">
                        <div class="join">
                            <span class="legend">Publisher</span>
                            <span class="xforms-help">
                                <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
