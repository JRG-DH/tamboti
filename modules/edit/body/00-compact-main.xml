                                </xf:input>
                            </xf:group>
                            <xf:group ref=".[@transliteration]" class="margin-plus text_100">
                                <xf:input ref=".">
                                    <xf:label>Transliterated</xf:label>
                                </xf:input>
                            </xf:group>
                        </xf:repeat>
                    </fieldset>
                </xf:group>
                <xf:group ref=".[mods:dateIssued]">
                    <fieldset class="level-2">
                        <div class="join">
                            <span class="legend">Date of Publication</span>
                            <span class="join-button">
                                <xf:trigger>
                                    <xf:label>+</xf:label>
                                    <xf:action ev:event="DOMActivate">
                                        <xf:insert nodeset="mods:dateIssued" at="index('dateIssued-repeat')" position="after" origin="instance('insert-templates')/mods:originInfo/mods:dateIssued"/>
                                    </xf:action>
                                </xf:trigger>
                            </span>
                            <span class="xforms-help">
                                <span onmouseover="XsltForms_browser.show(this, 'hint', true)" onmouseout="XsltForms_browser.show(this, 'hint', false)" class="xforms-hint-icon"/>
                                <div class="xforms-help-value">
                                    <xf:output mediatype="application/xhtml+xml" incremental="true" ref="instance('code-tables')/code-table[code-table-name='hint-code']/items/item[value='dateIssued-compact']/help"/>
