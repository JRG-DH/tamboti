                                <xf:input ref="." class="text_10 hide-label margin-plus">
                                    <xf:label>Date Issued</xf:label>
                                </xf:input>
                            </xf:group>
                            <!--dateIssued input ends-->
                        </xf:repeat>
                        <!--dateIssued repeat ends-->
                    </fieldset>
                </xf:group>
                <!--dateIssued ends-->
            </xf:repeat>
            <!--originInfo repeat ends-->

            <!--originInfo add begins-->

            <!--originInfo add ends-->
        </fieldset>
