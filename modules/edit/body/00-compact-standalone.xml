                                    <xf:output mediatype="application/xhtml+xml" incremental="true" ref="instance('code-tables')/code-table[code-table-name='hint-code']/items/item[value='place-compact']/help"/>
                                </div>
                            </span>
                        </div>
                        <!--place repeat begins-->
                        <xf:repeat nodeset="./mods:place" id="place-repeat">
                            <!--placeTerm begins-->
                            <xf:group ref=".[mods:placeTerm]">
                                <fieldset class="level-3">
                                    <!--placeTerm repeat begins-->
                                    <xf:repeat nodeset="./mods:placeTerm" id="place-term-repeat">
                                        <!--placeTerm input begins-->
                                        <xf:group ref=".[not(@transliteration)]" class="margin-plus">
                                            <xf:input ref="." class="text_100 hide-label">
