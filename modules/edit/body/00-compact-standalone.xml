                    </xf:input>
                </xf:group>
            </xf:repeat>
        </fieldset>
    </xf:group>
    <xf:group ref=".[not(/mods:mods/mods:extension/e:template != ('contribution-to-anthology-transliterated-compact','contribution-to-anthology-compact','book-review-latin','book-review-transliteration'))]">
