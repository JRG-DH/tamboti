                                </xf:input>
                            </xf:group>
                            <xf:group ref=".[@transliteration]" class="margin-plus">
                                <xf:input ref="." class="text_100">
                                    <xf:label>Transliterated</xf:label>
                                </xf:input>
                            </xf:group>
                            <!--publisher input ends-->
                        </xf:repeat>
                        <!--publisher repeat ends-->
                    </fieldset>
                </xf:group>
                <!--publisher ends-->

                <!--dateIssued begins-->
                <xf:group ref=".[mods:dateIssued]">
                    <fieldset class="level-2">
                        <div class="join">
