                                </fieldset>
                            </xf:group>
                            <!--placeTerm ends-->
                        </xf:repeat>
                        <!--place repeat ends-->
                    </fieldset>
                </xf:group>
                <!--place ends-->
                <!--publisher begins-->
                <xf:group ref=".[mods:publisher]">
                    <fieldset class="level-2">
                        <div class="join">
                            <span class="legend">Publisher</span>
                            <span class="xforms-help">
                                <span onmouseover="show(this, 'hint', true)" onmouseout="show(this, 'hint', false)" class="xforms-hint-icon"/>
