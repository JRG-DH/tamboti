    <xf:group ref=".[mods:url]">
        <fieldset class="level-1">
            <div class="join">
                <span class="legend">
                    <span class="legend">URL</span>
                </span>
                <span class="xforms-help">
                    <span onmouseover="show(this, 'hint', true)" onmouseout="show(this, 'hint', false)" class="xforms-hint-icon"/>
